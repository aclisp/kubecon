{
  "extends": "airbnb",
  "plugins": ["react"],
  "rules": {
    "no-console": 2,
    "max-statements": [2, 50],
    "max-depth": [2, 5],
    "max-nested-callbacks": [2, 3],
    "max-params": [2, 5],
    "complexity": [2, 10],
    "max-len": [2, 120, 4],
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-var": 1,
    "comma-dangle": 1,
    "object-shorthand": 1,
    "quotes": 1,
    "react/prefer-stateless-function": 0,
    "react/jsx-no-bind": 0
  }
}
