{
    "kind": "ReplicationController",
    "apiVersion": "v1",
    "metadata": {
        "name": "NEED INPUT"
    },
    "spec": {
        "replicas": 3,
        "selector": {
            "managed-by": "AUTO"
        },
        "template": {
            "metadata": {
                "labels": {
                    "managed-by": "AUTO"
                },
                "annotations": {
                    "USER_DEFINED_1": "VALUE_1",
                    "USER_DEFINED_2": "VALUE_2"
                }
            },
            "spec": {
                "nodeSelector": {
                    "project": "default"
                },
                "containers": [{
                    "name": "AUTO",
                    "image": "61.160.36.122:8080/default/myapp:1.0.0",
                    "ports": [{
                        "name": "ssh",
                        "containerPort": 22
                    }, {
                        "name": "app",
                        "containerPort": 3306
                    }],
                    "resources": {
                        "requests": {
                            "cpu": "1",
                            "memory": "2Gi"
                        },
                        "limits": {
                            "cpu": "10",
                            "memory": "20Gi"
                        }
                    },
                    "volumeMounts": [{
                        "name": "shared-volume",
                        "mountPath": "/etc/shared-volume"
                    }, {
                        "name": "hostinfo",
                        "mountPath": "/home/dspeak/yyms",
                        "readOnly": true
                    }, {
                        "name": "localtime",
                        "mountPath": "/etc/localtime",
                        "readOnly": true
                    }, {
                        "name": "yy-repos",
                        "mountPath": "/usr/local/i386",
                        "readOnly": true
                    }, {
                        "name": "yymp-agent-sock",
                        "mountPath": "/tmp/yymp.agent.sock"
                    }, {
                        "name": "reflection",
                        "mountPath": "/etc/reflection",
                        "readOnly": true
                    }, {
                        "name": "persist",
                        "mountPath": "/persist"
                    }, {
                        "name": "app-data",
                        "mountPath": "/var/lib/app-data"
                    }],
                    "env": [{
                        "name": "SSH_PUBLIC_KEY",
                        "value": "ssh-dss  dw_huanghao"
                    }, {
                        "name": "APP_DEFINED_1",
                        "value": "VALUE_1"
                    }]
                }, {
                    "name": "data-volume",
                    "image": "61.160.36.122:8080/default/myapp-config:1.0.0",
                    "volumeMounts": [{
                        "name": "shared-volume",
                        "mountPath": "/shared-volume"
                    }]
                }],
                "volumes": [{
                    "name": "shared-volume",
                    "emptyDir": {
                        "medium": ""
                    }
                }, {
                    "name": "hostinfo",
                    "hostPath": {
                        "path": "/home/dspeak/yyms"
                    }
                }, {
                    "name": "localtime",
                    "hostPath": {
                        "path": "/etc/localtime"
                    }
                }, {
                    "name": "yy-repos",
                    "hostPath": {
                        "path": "/usr/local/i386"
                    }
                }, {
                    "name": "yymp-agent-sock",
                    "hostPath": {
                        "path": "/tmp/yymp.agent.sock"
                    }
                }, {
                    "name": "reflection",
                    "downwardAPI": {
                        "items": [{
                            "path": "annotations",
                            "fieldRef": {
                                "fieldPath": "metadata.annotations"
                            }
                        }, {
                            "path": "labels",
                            "fieldRef": {
                                "fieldPath": "metadata.labels"
                            }
                        }, {
                            "path": "instancename",
                            "fieldRef": {
                                "fieldPath": "metadata.name"
                            }
                        }, {
                            "path": "projectname",
                            "fieldRef": {
                                "fieldPath": "metadata.namespace"
                            }
                        }]
                    }
                }, {
                    "name": "persist",
                    "emptyDir": {
                        "medium": ""
                    }
                }, {
                    "name": "app-data",
                    "emptyDir": {
                        "medium": ""
                    }
                }]
            }
        }
    }
}
